{
  "name": "vuex-plugin-jsdata",
  "version": "0.1.0",
  "description": "A vuex plugin to sync vuex store with jsdata-store",
  "main": "dist/vuex-jsdata-plugin.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rollup -c --name vuexjsdataplugin --input index.js --output dist/vuex-jsdata-plugin.js --format umd",
    "serve": "NODE_PATH=$NODE_PATH:examples/lib budo examples/lib/index.js --open -d . --live -- -t [ vueify ] -t [ babelify --presets [ es2015 ] ]",
    "dist": "NODE_PATH=$NODE_PATH:examples/lib browserify -e examples/lib/index.js -o dist/index.js -t vueify -t [ babelify --presets [ es2015 ] ] -t uglifyify -p [ minifyify --no-map ]"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlexandreBonaventure/vuex-jsdata-plugin.git"
  },
  "keywords": [
    "vuex",
    "js-data",
    "plugin",
    "store",
    "model",
    "data"
  ],
  "author": "Alexandre Bonaventure Geissmann",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AlexandreBonaventure/vuex-jsdata-plugin/issues"
  },
  "homepage": "https://github.com/AlexandreBonaventure/vuex-jsdata-plugin#readme",
  "peerDependencies": {
    "vue": ">= 1.x <= 2.x"
  },
  "dependencies": {
    "lodash": "^4.13.1"
  },
  "devDependencies": {
    "babel-plugin-external-helpers": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babelify": "^7.3.0",
    "babelify-external-helpers": "^1.1.0",
    "browserify": "^13.1.1",
    "browserify-shim": "^3.8.12",
    "budo": "^9.2.2",
    "bundle-collapser": "^1.2.1",
    "jade": "^1.11.0",
    "js-data": "^2.9.0",
    "lodash": "^3.10.1",
    "minifyify": "^7.3.4",
    "node-sass": "^4.2.0",
    "rollup": "^0.36.1",
    "rollup-plugin-babel": "^2.6.1",
    "uglifyify": "^3.0.4",
    "vue": "^2.1.8",
    "vue-hot-reload-api": "^1.3.3",
    "vueify": "^9.4.0",
    "vueify-insert-css": "^1.0.0",
    "vuex": "^2.0.0"
  },
  "browserify-shim": {}
}
